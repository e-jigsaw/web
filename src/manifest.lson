manifest_version: 2
name: \web-recorder
description: 'Fastest web recorder'
version: \0.0.1
author: 'jigsaw (http://jgs.me)'
\content_security_policy : "script-src 'unsafe-eval'; object-src 'self';"
permissions: <[tabs <all_urls>]>
background:
  scripts: <[event.js]>
  persistent: false
browser_action:
  default_popup: \index.html
